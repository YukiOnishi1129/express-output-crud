{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "nodemon ./dist/index.js",
    "build": "npx tsc",
    "build:watch": "npx tsc",
    "dev": "nodemon --watch 'src/**/*' --exec 'ts-node' src/index.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:generate": "npm run typeorm -- migration:generate src/database/migrations/$NAME  --dataSource src/config/migrationDataSource.ts",
    "migration:run": "npm run typeorm -- migration:run --dataSource src/config/migrationDataSource.ts",
    "migration:revert": "npm run typeorm -- migration:revert --dataSource src/config/migrationDataSource.ts",
    "seed:run": "ts-node ./src/database/seeders/index.ts",
    "migration:reset": "npm run migration:revert && npm run migration:run && npm run seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "mysql2": "^3.11.3",
    "reflect-metadata": "^0.2.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vitest": "^2.1.2"
  }
}
